<!DOCTYPE html>
<html lang="<%= lang %>">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title> Masters Way - <%= title %> </title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">

    <!-- jQuery-->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>

    <!-- Bootstrap & Normalize-->
    <link rel="stylesheet" href="/css/normalize.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>

    <link rel="stylesheet" href="/css/main.css">

    <link rel="stylesheet" href="/css/layout/main.css">
    <link rel="stylesheet" href="/css/template/<%= template %>.css">

    <!-- Socket.io -->
    <script src="/socket.io/socket.io.js"></script>

    <script src="/js/main.js"></script>
    <script src="/js/layout/main.js"></script>
    <script src="/js/template/<%= template %>.js"></script>

    <link rel="icon" type="image/png" href="/favicon.png">
</head>
<body>

<div id="wrapper">
    <header class="clearfix">
        <div class="col-md-9">
            <div class="row">
                <h1 class="no-style col-md-3">
                    <a href="/" class="no-style"
                        <% if( __('game-name') != 'Masters Way') { %>
                            title="<%- __('game-name') %>"
                        <% } %>
                    >
                        Masters Way
                    </a>
                </h1>

                <div class="col-md-9">
                    <form method="get">

                        <button type="submit" class="no-style" name="lang" value="ua">
                            <img src="data:image/png;base64,"/>
                        </button>
                        <button type="submit" class="no-style" name="lang" value="ru">
                            <img src="data:image/png;base64,"/>
                        </button>
                        <button type="submit" class="no-style" name="lang" value="en">
                            <img src="data:image/png;base64,"/>
                        </button>
                    </form>
                </div>
            </div>
        </div>

        <ol class="col-md-1 no-style">

            <li>
                <a href="http://slave-maker-online.blogspot.com/" class="font-icon icon-blogger" title="blogger"></a>
            </li>
            <li>
                <a href="https://twitter.com/MastersWayGame" class="font-icon icon-twitter" title="twitter"></a>
            </li>
        </ol>

        <ol class="col-md-2 no-style">
            <li>
                <a href="/mythology">
                    <%- __('site-menu: mythology') %>
                </a>
            </li>
            <li>
                <a href="/faq">
                    <%- __('site-menu: faq') %>
                </a>
            </li>
        </ol>
    </header>
    <div id="content" class="row">
        <%- content %>
    </div>
    <footer class="clearfix text-center">
        <a href="mailto:support@masters-way.sx">support@masters-way.sx</a>
    </footer>
</div>
</body>
</html>